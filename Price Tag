--Price Tag

local s = Instance.new("Sound")

s.Name = "Sound"
s.SoundId = "http://www.roblox.com/asset/?id=6783361286"
s.Volume = 100
s.Looped = true
s.archivable = false

s.Parent = game.Workspace

wait(3)

s:play()

